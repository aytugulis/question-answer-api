# Server Variables
PORT=5000
NODE_ENV=development

# MongoDb Connection
MONGO_URI=<YOUR_MONGO_URI>

# Json Web Token
JWT_SECRET_KEY=<YOUR_SECRET_KEY>
JWT_EXPIRE=10m

# Cookie
JWT_COOKIE=10

# Reset Password
RESET_PASSWORD_EXPIRE=3600000

# NodeMailer
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=<YOUR_GMAIL_EMAIL>
SMTP_PASS=<YOUR_GMAIL_PASSWORD>
